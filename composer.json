{
  "name": "birjemin/curl",
  "description": "一个轻量级的PHP网络操作类，实现GET、POST、PUT、DELETE常用操作，出错自动重试，支持链式写法",
  "keywords": [
    "php curl",
    "curl post",
    "curl put",
    "curl delete",
    "curl library",
    "curl lightweight"
  ],
  "homepage": "https://github.com/birjemin/curl",
  "type": "library",
  "require": {
    "php": ">=5.6",
    "ext-curl": "*"
  },
  "license": "MIT",
  "authors": [
    {
      "name": "Wenpeng",
      "email": "imwwp@outlook.com"
    }, {
      "name": "birjemin",
      "email": "birjemin@foxmail.com"
    }
  ],
  "autoload": {
    "psr-4": {
      "Birjemin\\Curl\\": "src/"
    }
  },
  "extra": {
    "branch-alias": {
      "dev-master": "0.0.4-dev"
    }
  },
  "minimum-stability": "dev",
  "prefer-stable": true
}
